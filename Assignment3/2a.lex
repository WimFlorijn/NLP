; VERBS
;   VBP:    Verb, non-3rd person singular present
;   VBZ:    Verb, 3rd person singular present

\w research
\c VBP

\w fans
\c VBZ
  
\w hope
\c VBP

; NOUNS
;   NN:     Noun, singular or mass
;   NNS:    Noun, plural

\w research
\c NN

\w fans
\c NNS

\w hope
\c NN

\w injuries
\c NNS

; PREPOSITIONS
;   IN:     Preposition or subordinating conjunction

\w for
\c IN

; ADJECTIVES
;   JJ: Adjective

\w spinal
\c JJ